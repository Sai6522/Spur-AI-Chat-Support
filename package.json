{
  "name": "spur-chat-agent",
  "version": "1.0.0",
  "description": "AI Live Chat Agent for Spur take-home assignment",
  "main": "dist/server.js",
  "scripts": {
    "dev:backend": "tsx watch src/server.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "build:backend": "tsc",
    "build:frontend": "cd frontend && npm run build",
    "build": "npm run build:backend && npm run build:frontend",
    "start": "node dist/server.js",
    "db:setup": "tsx src/scripts/setup-db.ts",
    "postinstall": "cd frontend && npm install",
    "render:build": "./render-build.sh"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@vercel/node": "^5.5.16",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
